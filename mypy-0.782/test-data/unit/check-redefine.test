    reveal_type(x) # N: Revealed type is 'builtins.object'
def g() -> None:
    x = 1
    reveal_type(x) # N: Revealed type is 'builtins.int'
    x: object = ''
    reveal_type(x) # N: Revealed type is 'builtins.object'
def h() -> None:
    x: int
    x = 1
    reveal_type(x) # N: Revealed type is 'builtins.int'
    x: object
